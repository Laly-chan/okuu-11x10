#include<windows.h>
#include<iostream>
#include<conio.h>

void okuu();
void gotoxy(int, int);

int main(){
	
	okuu();
	
	getch();
	return 0;
}

void gotoxy(int x1, int y1){
	HANDLE hCon;
	COORD dwPos;
	
	dwPos.X = x1;
	dwPos.Y = y1;
	hCon = GetStdHandle(STD_OUTPUT_HANDLE);
	SetConsoleCursorPosition(hCon,dwPos);
}

